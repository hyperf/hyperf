{
    "name": "hyperf/session",
    "description": "A session library",
    "license": "MIT",
    "keywords": [
        "php",
        "session"
    ],
    "homepage": "https://hyperf.io",
    "support": {
        "issues": "https://github.com/hyperf/hyperf/issues",
        "source": "https://github.com/hyperf/hyperf",
        "docs": "https://hyperf.wiki",
        "pull-request": "https://github.com/hyperf/hyperf/pulls"
    },
    "require": {
        "php": ">=8.2",
        "hyperf/collection": "~3.2.0",
        "hyperf/support": "~3.2.0",
        "hyperf/utils": "~3.2.0",
        "psr/http-server-middleware": "^1.0"
    },
    "suggest": {
        "hyperf/config": "Required to load session config.",
        "psr/container": "Required to use SessionFactory."
    },
    "autoload": {
        "psr-4": {
            "Hyperf\\Session\\": "src/"
        }
    },
    "autoload-dev": {
        "psr-4": {
            "HyperfTest\\Session\\": "tests/"
        }
    },
    "config": {
        "sort-packages": true
    },
    "extra": {
        "branch-alias": {
            "dev-master": "3.1-dev"
        },
        "hyperf": {
            "config": "Hyperf\\Session\\ConfigProvider"
        }
    }
}
