{
    "name": "hyperf/testing",
    "type": "library",
    "license": "MIT",
    "keywords": [
        "php",
        "swoole",
        "testing"
    ],
    "description": "Testing for hyperf",
    "autoload": {
        "psr-4": {
            "Hyperf\\Testing\\": "src/"
        }
    },
    "autoload-dev": {
        "psr-4": {
            "HyperfTest\\Testing\\": "tests/"
        }
    },
    "require": {
        "php": ">=7.2",
        "psr/container": "^1.0",
        "phpunit/phpunit": "^9.4",
        "hyperf/contract": "~2.1.0",
        "hyperf/http-message": "~2.1.0",
        "hyperf/http-server": "~2.1.0",
        "hyperf/utils": "~2.1.0"
    },
    "require-dev": {
        "malukenho/docheader": "^0.1.6",
        "mockery/mockery": "^1.0",
        "friendsofphp/php-cs-fixer": "^2.9"
    },
    "suggest": {
    },
    "extra": {
        "branch-alias": {
            "dev-master": "2.1-dev"
        }
    },
    "bin": [
        "co-phpunit"
    ],
    "scripts": {
        "test": "./vendor/bin/phpunit -c phpunit.xml",
        "co_test": "php tests/co_phpunit.php -c phpunit.xml"
    }
}
