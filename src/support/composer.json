{
    "name": "hyperf/support",
    "description": "A support component for Hyperf.",
    "license": "MIT",
    "keywords": [
        "php",
        "swoole",
        "hyperf",
        "support"
    ],
    "homepage": "https://hyperf.io",
    "support": {
        "docs": "https://hyperf.wiki",
        "issues": "https://github.com/hyperf/hyperf/issues",
        "pull-request": "https://github.com/hyperf/hyperf/pulls",
        "source": "https://github.com/hyperf/hyperf"
    },
    "require": {
        "php": ">=8.0",
        "hyperf/collection": "~3.0.0",
        "hyperf/context": "~3.0.0",
        "hyperf/contract": "~3.0.0",
        "hyperf/coroutine": "~3.0.0",
        "hyperf/macroable": "~3.0.0",
        "hyperf/stringable": "~3.0.0"
    },
    "suggest": {
        "nesbot/carbon": "Use Carbon as DateTime object.(^2.0)"
    },
    "autoload": {
        "psr-4": {
            "Hyperf\\Support\\": "src/"
        },
        "files": [
            "src/Functions.php"
        ]
    },
    "autoload-dev": {
        "psr-4": {
            "HyperfTest\\Support\\": "tests/"
        }
    },
    "config": {
        "sort-packages": true
    },
    "extra": {
        "branch-alias": {
            "dev-master": "3.0-dev"
        }
    }
}
