{
    "name": "hyperf/config-aliyun-acm",
    "description": "An aliyun acm adapter for Hyperf config component.",
    "license": "MIT",
    "keywords": [
        "php",
        "swoole",
        "hyperf",
        "config",
        "configuration",
        "aliyun",
        "acm"
    ],
    "homepage": "https://hyperf.io",
    "support": {
        "docs": "https://hyperf.wiki",
        "issues": "https://github.com/hyperf/hyperf/issues",
        "pull-request": "https://github.com/hyperf/hyperf/pulls",
        "source": "https://github.com/hyperf/hyperf"
    },
    "require": {
        "php": ">=8.0",
        "hyperf/codec": "~3.0.0",
        "hyperf/config-center": "~3.0.0",
        "hyperf/contract": "~3.0.0",
        "hyperf/guzzle": "~3.0.0",
        "psr/container": "^1.0|^2.0"
    },
    "autoload": {
        "psr-4": {
            "Hyperf\\ConfigAliyunAcm\\": "src/"
        }
    },
    "config": {
        "sort-packages": true
    },
    "extra": {
        "branch-alias": {
            "dev-master": "3.0-dev"
        },
        "hyperf": {
            "config": "Hyperf\\ConfigAliyunAcm\\ConfigProvider"
        }
    }
}
