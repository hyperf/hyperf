{
    "name": "hyperf/crontab",
    "description": "A crontab component for Hyperf.",
    "license": "MIT",
    "keywords": [
        "php",
        "swoole",
        "hyperf",
        "crontab"
    ],
    "homepage": "https://hyperf.io",
    "support": {
        "docs": "https://hyperf.wiki",
        "issues": "https://github.com/hyperf/hyperf/issues",
        "pull-request": "https://github.com/hyperf/hyperf/pulls",
        "source": "https://github.com/hyperf/hyperf"
    },
    "require": {
        "php": ">=8.0",
        "hyperf/framework": "~3.0.0",
        "hyperf/utils": "~3.0.0",
        "hyperf/support": "~3.0.0",
        "nesbot/carbon": "^2.0"
    },
    "autoload": {
        "psr-4": {
            "Hyperf\\Crontab\\": "src/"
        }
    },
    "autoload-dev": {
        "psr-4": {
            "HyperfTest\\Crontab\\": "tests/"
        }
    },
    "suggest": {
        "hyperf/process": "Auto register the Crontab process for server.",
        "hyperf/command": "Required to use command trigger."
    },
    "config": {
        "sort-packages": true
    },
    "extra": {
        "branch-alias": {
            "dev-master": "3.0-dev"
        },
        "hyperf": {
            "config": "Hyperf\\Crontab\\ConfigProvider"
        }
    }
}
