{
    "name": "hyperf/etcd",
    "description": "Etcd Client for Hyperf.",
    "license": "MIT",
    "type": "library",
    "keywords": [
        "php",
        "hyperf",
        "etcd"
    ],
    "require": {
        "php": ">=8.2",
        "hyperf/guzzle": "~3.2.0",
        "hyperf/support": "~3.2.0",
        "hyperf/utils": "~3.2.0",
        "jetbrains/phpstorm-attributes": "^1.0"
    },
    "autoload": {
        "psr-4": {
            "Hyperf\\Etcd\\": "src/"
        }
    },
    "autoload-dev": {
        "psr-4": {
            "HyperfTest\\Etcd\\": "tests/"
        }
    },
    "config": {
        "sort-packages": true
    },
    "extra": {
        "branch-alias": {
            "dev-master": "3.1-dev"
        },
        "hyperf": {
            "config": "Hyperf\\Etcd\\ConfigProvider"
        }
    }
}
