{
    "name": "hyperf/db",
    "description": "",
    "license": "MIT",
    "type": "library",
    "keywords": [
        "php",
        "hyperf"
    ],
    "require": {
        "php": ">=8.2",
        "hyperf/config": "~3.2.0",
        "hyperf/contract": "~3.2.0",
        "hyperf/pool": "~3.2.0",
        "hyperf/support": "~3.2.0",
        "hyperf/utils": "~3.2.0",
        "psr/container": "^1.0 || ^2.0"
    },
    "suggest": {
        "ext-swoole": "Required to use Swoole Coroutine MySQL."
    },
    "autoload": {
        "psr-4": {
            "Hyperf\\DB\\": "src/"
        }
    },
    "autoload-dev": {
        "psr-4": {
            "HyperfTest\\DB\\": "tests/"
        }
    },
    "config": {
        "sort-packages": true
    },
    "extra": {
        "branch-alias": {
            "dev-master": "3.1-dev"
        },
        "hyperf": {
            "config": "Hyperf\\DB\\ConfigProvider"
        }
    }
}
