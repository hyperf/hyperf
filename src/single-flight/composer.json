{
    "name": "hyperf/single-flight",
    "description": "A single-flight library for Hyperf",
    "type": "library",
    "license": "MIT",
    "keywords": [
        "php",
        "hyperf",
        "single-flight"
    ],
    "homepage": "https://hyperf.io",
    "support": {
        "issues": "https://github.com/hyperf/hyperf/issues",
        "source": "https://github.com/hyperf/hyperf",
        "docs": "https://hyperf.wiki",
        "pull-request": "https://github.com/hyperf/hyperf/pulls"
    },
    "autoload": {
        "psr-4": {
            "Hyperf\\SingleFlight\\": "src/"
        }
    },
    "require": {
        "php": ">=8.1",
        "hyperf/di": "~3.1.0",
        "hyperf/support": "~3.1.0"
    },
    "autoload-dev": {
        "psr-4": {
            "HyperfTest\\SingleFlight\\": "tests/"
        }
    },
    "config": {
        "sort-packages": true
    },
    "extra": {
        "hyperf": {
            "config": "Hyperf\\SingleFlight\\ConfigProvider"
        }
    }
}
