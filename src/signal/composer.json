{
    "name": "hyperf/signal",
    "description": "A signal library for Hyperf.",
    "license": "MIT",
    "keywords": [
        "php",
        "swoole",
        "hyperf",
        "signal"
    ],
    "homepage": "https://hyperf.io",
    "support": {
        "issues": "https://github.com/hyperf/hyperf/issues",
        "source": "https://github.com/hyperf/hyperf",
        "docs": "https://hyperf.wiki",
        "pull-request": "https://github.com/hyperf/hyperf/pulls"
    },
    "require": {
        "php": ">=8.2",
        "hyperf/contract": "~3.2.0",
        "hyperf/engine": "^2.0",
        "hyperf/stdlib": "~3.2.0",
        "hyperf/support": "~3.2.0",
        "hyperf/utils": "~3.2.0",
        "psr/container": "^1.0 || ^2.0"
    },
    "autoload": {
        "psr-4": {
            "Hyperf\\Signal\\": "src/"
        }
    },
    "autoload-dev": {
        "psr-4": {
            "HyperfTest\\Signal\\": "tests/"
        }
    },
    "config": {
        "sort-packages": true
    },
    "extra": {
        "branch-alias": {
            "dev-master": "3.1-dev"
        },
        "hyperf": {
            "config": "Hyperf\\Signal\\ConfigProvider"
        }
    }
}
