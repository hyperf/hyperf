{
    "name": "hyperf/elasticsearch",
    "description": "Elasticsearch client for hyperf",
    "license": "MIT",
    "type": "library",
    "keywords": [
        "php",
        "swoole",
        "elasticsearch"
    ],
    "require": {
        "php": ">=8.2",
        "elasticsearch/elasticsearch": "^8.0",
        "hyperf/guzzle": "~3.2.0"
    },
    "suggest": {
        "ezimuel/ringphp": "Guzzle RingPHP adapter to allow for custom handling of requests and responses when require elasticsearch/elasticsearch 7.x.(^1.1.2)"
    },
    "autoload": {
        "psr-4": {
            "Hyperf\\Elasticsearch\\": "src/"
        }
    },
    "autoload-dev": {
        "psr-4": {
            "HyperfTest\\Elasticsearch\\": "tests/"
        }
    },
    "config": {
        "sort-packages": true
    },
    "extra": {
        "branch-alias": {
            "dev-master": "3.1-dev"
        }
    }
}
