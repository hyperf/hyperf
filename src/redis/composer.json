{
    "name": "hyperf/redis",
    "description": "A redis component for hyperf.",
    "license": "MIT",
    "keywords": [
        "php",
        "hyperf",
        "redis",
        "pool"
    ],
    "homepage": "https://hyperf.io",
    "support": {
        "issues": "https://github.com/hyperf/hyperf/issues",
        "source": "https://github.com/hyperf/hyperf",
        "docs": "https://hyperf.wiki",
        "pull-request": "https://github.com/hyperf/hyperf/pulls"
    },
    "require": {
        "php": ">=8.2",
        "ext-redis": "^5.0 || ^6.0",
        "hyperf/contract": "~3.2.0",
        "hyperf/pool": "~3.2.0",
        "hyperf/support": "~3.2.0",
        "hyperf/tappable": "~3.2.0",
        "hyperf/utils": "~3.2.0",
        "psr/container": "^1.0 || ^2.0"
    },
    "suggest": {
        "ext-redis": "Required to use sentinel mode (>=5.2).",
        "hyperf/di": "Create the RedisPool via dependency injection."
    },
    "autoload": {
        "psr-4": {
            "Hyperf\\Redis\\": "src/"
        }
    },
    "autoload-dev": {
        "psr-4": {
            "HyperfTest\\Redis\\": "tests/"
        }
    },
    "config": {
        "sort-packages": true
    },
    "extra": {
        "branch-alias": {
            "dev-master": "3.1-dev"
        },
        "hyperf": {
            "config": "Hyperf\\Redis\\ConfigProvider"
        }
    }
}
