{
    "name": "hyperf/grpc-server",
    "description": "A GRPC Server for Hyperf.",
    "license": "MIT",
    "keywords": [
        "php",
        "swoole",
        "hyperf",
        "grpc",
        "grpc-server"
    ],
    "homepage": "https://hyperf.io",
    "support": {
        "issues": "https://github.com/hyperf/hyperf/issues",
        "source": "https://github.com/hyperf/hyperf",
        "docs": "https://hyperf.wiki",
        "pull-request": "https://github.com/hyperf/hyperf/pulls"
    },
    "require": {
        "php": ">=8.1",
        "hyperf/di": "~3.1.0",
        "hyperf/grpc": "~3.1.0",
        "hyperf/http-message": "~3.1.0",
        "hyperf/http-server": "~3.1.0",
        "hyperf/serializer": "~3.1.0",
        "hyperf/support": "~3.1.0",
        "hyperf/utils": "~3.1.0",
        "swow/psr7-plus": "^1.0"
    },
    "autoload": {
        "psr-4": {
            "Hyperf\\GrpcServer\\": "src/"
        }
    },
    "autoload-dev": {
        "psr-4": {
            "HyperfTest\\GrpcServer\\": "tests/"
        }
    },
    "config": {
        "sort-packages": true
    },
    "extra": {
        "branch-alias": {
            "dev-master": "3.1-dev"
        },
        "hyperf": {
            "config": "Hyperf\\GrpcServer\\ConfigProvider"
        }
    }
}
