{
    "name": "hyperf/swagger",
    "description": "A swagger library for Hyperf.",
    "license": "MIT",
    "keywords": [
        "php",
        "swoole",
        "hyperf",
        "swagger"
    ],
    "homepage": "https://hyperf.io",
    "support": {
        "issues": "https://github.com/hyperf/hyperf/issues",
        "source": "https://github.com/hyperf/hyperf",
        "docs": "https://hyperf.wiki",
        "pull-request": "https://github.com/hyperf/hyperf/pulls"
    },
    "require": {
        "php": ">=8.2",
        "hyperf/codec": "~3.2.0",
        "hyperf/command": "~3.2.0",
        "hyperf/context": "~3.2.0",
        "hyperf/contract": "~3.2.0",
        "hyperf/engine": "^2.0",
        "hyperf/stringable": "~3.2.0",
        "zircote/swagger-php": "^4.6"
    },
    "suggest": {
        "hyperf/validation": "Required to use SwaggerRequest."
    },
    "autoload": {
        "psr-4": {
            "Hyperf\\Swagger\\": "src/"
        }
    },
    "autoload-dev": {
        "psr-4": {
            "HyperfTest\\Swagger\\": "tests/"
        }
    },
    "config": {
        "sort-packages": true
    },
    "extra": {
        "branch-alias": {
            "dev-master": "3.1-dev"
        },
        "hyperf": {
            "config": "Hyperf\\Swagger\\ConfigProvider"
        }
    }
}
